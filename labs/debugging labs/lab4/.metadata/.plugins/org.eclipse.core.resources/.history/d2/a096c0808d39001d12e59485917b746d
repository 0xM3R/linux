#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) 
{
	char *p;
	char *q = "hello world and have a nice day";
	char *r;
	
	p=malloc(4);
	strcpy(p,q);
	puts(p);
	r=malloc(4);
	
	puts(p);
	if(r == NULL)
		puts("failed");
	
	
	return EXIT_SUCCESS;
}
