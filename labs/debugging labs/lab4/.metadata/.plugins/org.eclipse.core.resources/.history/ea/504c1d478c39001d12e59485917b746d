#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) 
{
	char *p;
	char *q = "hello world";
	char *r;
	
	p=malloc(strlen(q + 4));
	strcpy(p,q);
	r=malloc(strlen(q) + 1);
	
	puts(p);
	if(r == NULL)
		puts("failed");
	
	
	return EXIT_SUCCESS;
}
